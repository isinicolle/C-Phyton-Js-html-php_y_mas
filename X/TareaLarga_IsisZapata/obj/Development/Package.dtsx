join [HumanResources].[Employee] E on E.EmployeeID= DH.EmployeeID 
join [Person].[Contact] C on E.EmployeeID= C.ContactID
join [Sales].[SalesOrderHeader] SOH on SOH.ContactID= C.ContactID
